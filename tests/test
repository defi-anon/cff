// initial balances before block
0 in 0 gets 50000000000000000000000000000000000000000000;
0 in 1 gets 100000000000000000000000000000000000000000000;
1 in 0 gets 100000000000000000000000000000000000000000000;
2 in 1 gets 200000000000000000000000000000000000000000000;
// setup 100 balances; special address 100
100 in 0 gets 100000000000000000000000;
100 in 1 gets 500000000000000000000000;

// transactions performed during interval (reorgable)
0 in 0 swaps 50000000000000000000000 input for 0 fee 0;
2 in 1 swaps 100000000000000000000000 for 0 output fee 0;
2 in 0 swaps 50000000000000000000000 for 1 output fee 0;
2 in 1 swaps 10000000000000000000000 for 0 output fee 0;
0 in 1 swaps 20000000000000000000000 for 0 output fee 0;

